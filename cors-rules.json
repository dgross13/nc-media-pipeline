[
    {
        "corsRuleName": "downloadFromAnyOrigin",
        "allowedOrigins": ["*"],
        "allowedHeaders": ["*"],
        "allowedOperations": ["b2_download_file_by_name"],
        "exposeHeaders": ["x-bz-content-sha1"],
        "maxAgeSeconds": 3600
    },
    {
        "corsRuleName": "uploadFromWebApp",
        "allowedOrigins": ["https://*.azurestaticapps.net", "http://localhost:*"],
        "allowedHeaders": ["*"],
        "allowedOperations": ["b2_upload_file", "b2_upload_part"],
        "exposeHeaders": ["x-bz-content-sha1", "x-bz-upload-timestamp"],
        "maxAgeSeconds": 3600
    }
]
