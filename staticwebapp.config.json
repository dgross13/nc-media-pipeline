{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/upload/*",
      "methods": ["GET", "POST"],
      "rewrite": "/api/upload"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
